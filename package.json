{
  "name": "frame-breaker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "forge test",
    "test:coverage": "forge coverage",
    "compile": "forge build",
    "deploy:base": "forge script blockchain/scripts/DeployBase.s.sol --rpc-url base --broadcast --verify -vvvv",
    "deploy:baseSepolia": "forge script blockchain/scripts/DeployBase.s.sol --rpc-url baseSepolia --broadcast --verify -vvvv",
    "deploy:monad": "forge script blockchain/scripts/DeployMonad.s.sol --rpc-url monad --broadcast --verify -vvvv",
    "deploy:monadTestnet": "forge script blockchain/scripts/DeployMonad.s.sol --rpc-url monadTestnet --broadcast --verify -vvvv",
    "verify": "forge verify-contract",
    "keystore:create": "python keystore_import.py"
  },
  "dependencies": {
    "@farcaster/miniapp-sdk": "^0.1.9",
    "@google/genai": "^1.17.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "viem": "^2.27.2",
    "wagmi": "^2.16.0"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.9.6",
    "@types/node": "^20.19.14",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^5.3.3"
  },
  "description": "An AI-powered brick breaker game with retro aesthetics and modern features, built as a Farcaster Mini App. Each level is dynamically generated using Google's Gemini AI to create unique, challenging patterns.",
  "main": "eslint.config.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdeeks/frame-breaker-85.git"
  },
